<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/themeRed</item>
    </style>
    <style name="Widget.BottomNavigationView.TextStyle"
        parent="Theme.DatingApp">
        <item name="fontFamily">@font/urbanist_medium</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">bottom</item>
    </style>

    <style name="TopCornersRound20" parent="">
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerSizeTopLeft">20dp</item>
        <item name="cornerSizeTopRight">20dp</item>
    </style>
    <style name="CornersRound10" parent="">
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerSizeTopLeft">10dp</item>
        <item name="cornerSizeTopRight">10dp</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerSizeBottomLeft">10dp</item>
        <item name="cornerSizeBottomRight">10dp</item>
    </style>

    <style name="CornersRound20" parent="">
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerSizeTopLeft">20dp</item>
        <item name="cornerSizeTopRight">20dp</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerSizeBottomLeft">20dp</item>
        <item name="cornerSizeBottomRight">20dp</item>
    </style>
    <style name="CornersRound14" parent="">
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerSizeTopLeft">14dp</item>
        <item name="cornerSizeTopRight">14dp</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerSizeBottomLeft">14dp</item>
        <item name="cornerSizeBottomRight">14dp</item>
    </style>
    <style name="MatchedLeft" parent="">
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerSizeTopLeft">30dp</item>
        <item name="cornerSizeTopRight">25dp</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerSizeBottomLeft">30dp</item>
        <item name="cornerSizeBottomRight">25dp</item>
    </style>
    <style name="MatchedRight" parent="">
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerSizeTopLeft">25dp</item>
        <item name="cornerSizeTopRight">30dp</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerSizeBottomLeft">25dp</item>
        <item name="cornerSizeBottomRight">30dp</item>
    </style>
    <style name="CornerSize50Percent" parent="">
        <item name="cornerSize">50%</item>
    </style>

    <style name="CornersRound16" parent="">
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerSizeTopLeft">16dp</item>
        <item name="cornerSizeTopRight">16dp</item>
        <item name="cornerSizeBottomLeft">16dp</item>
        <item name="cornerSizeBottomRight">16dp</item>
    </style>
    <style name="CornersRound12" parent="">
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerSizeTopLeft">12.5dp</item>
        <item name="cornerSizeTopRight">12.5dp</item>
        <item name="cornerSizeBottomLeft">12.5dp</item>
        <item name="cornerSizeBottomRight">12.5dp</item>
    </style>

    <style name="CornersRound15" parent="">
        <item name="cornerFamilyTopLeft">rounded</item>
        <item name="cornerFamilyTopRight">rounded</item>
        <item name="cornerFamilyBottomLeft">rounded</item>
        <item name="cornerFamilyBottomRight">rounded</item>
        <item name="cornerSizeTopLeft">15dp</item>
        <item name="cornerSizeTopRight">15dp</item>
        <item name="cornerSizeBottomLeft">15dp</item>
        <item name="cornerSizeBottomRight">15dp</item>
    </style>

    <style name="ChipTextStyleSelected" parent="">
        <item name="fontFamily">@font/sk_modernist_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingStart">15dp</item>
        <item name="android:paddingEnd">15dp</item>
        <item name="android:layout_marginEnd">10dp</item>
    </style>

    <style name="ChipTextStyleUnSelected" parent="">
        <item name="fontFamily">@font/sk_modernist_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingStart">15dp</item>
        <item name="android:paddingEnd">15dp</item>
        <item name="android:layout_marginEnd">10dp</item>
    </style>
    <style name="ChipCustomStyle" parent="Widget.MaterialComponents.Chip.Action">
        <item name="checkedIconEnabled">false</item>
        <item name="checkedIcon">@null</item>
    </style>
    <style name="CustomRangeSlider" parent="Widget.MaterialComponents.Slider">
        <item name="trackColorInactive">@color/color_D4D4D4</item>
        <item name="trackColorActive">@color/color_FFC629</item>
    </style>
    <style name="FullScreenBottomSheetDialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowFullscreen">true</item>
    </style>
</resources>